fat_tree: escalonador executa_postergado gerente shutdown


escalonador: escalonador.cpp
	g++ escalonador.cpp -Wall -o escalonador

executa_postergado: executa_postergado.cpp
	g++ executa_postergado.cpp -Wall -o executa_postergado

gerente: gerente.cpp
	g++ gerente.cpp -Wall -o gerente

shutdown: shutdown.cpp
	g++ shutdown.cpp -Wall -o shutdown

clean: 
	rm -f escalonador executa_postergado gerente shutdown *.o